<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@android:color/transparent</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimaryDark</item>

        <!--<item name="android:colorBackground">@color/colorBack</item>-->
    </style>
        <style name="CustomTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="colorPrimary">@android:color/transparent</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimaryDark</item>
    </style>
    <style name="PlacesTheme" parent="Theme.AppCompat.DayNight.Dialog.MinWidth">
        <item name="colorPrimary">@color/colorPrimaryDark</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/thunder</item>
    </style>

    <style name="SplashTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowActionBarOverlay">@null</item>

    </style>
    <style name="SettingsTheme" parent="Theme.AppCompat">
        <item name="colorPrimary">#fd5e53</item>
        <item name="colorAccent">#fd5e53</item>
        <item name="colorPrimaryDark">#fd5e53</item>


    </style>

</resources>
